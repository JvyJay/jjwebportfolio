{"ast":null,"code":"// IMAGES\nimport project1 from '../assets/project1.png';\nimport project2 from '../assets/project2.png';\nimport project3 from '../assets/project3.png';\nimport project4 from '../assets/project4.png';\nimport code from '../assets/Code.svg';\nimport mail from '../assets/mail.svg';\nimport designbanner from '../assets/design-banner.svg';\nimport friendspicks from '../assets/FriendsPicks.svg';\n// COMPONENTS\nimport ProjectCard from '../components/Project-Card.vue';\nimport FooterNav from '../components/FooterNav.vue';\nexport default {\n  name: \"HomeView\",\n  components: {\n    ProjectCard,\n    FooterNav\n  },\n  data() {\n    return {\n      // IMAGE DATA\n      code: code,\n      mail: mail,\n      designbanner: designbanner,\n      friendspicks: friendspicks,\n      // PROJECT DATA\n      projects: [{\n        id: 1,\n        title: 'Web Agency',\n        image: project1,\n        desc: 'Personal Project: Local Seattle agency inspired me, so I felt like doing a redesign of their website.',\n        link: 'https://web-agency-vs7d.vercel.app/'\n      }, {\n        id: 2,\n        title: 'Achilles Collective',\n        image: project2,\n        desc: `Personal Project: A Meetup Group passion project that's one of a couple reasons why I'm trying so hard to get into the I.T industry at the moment; still in the works, but I plan on investing more into this project as a whole once I'm more settled into my career (please hire me lol (I'll train you for free!!))`,\n        link: 'https://achilles-collective.vercel.app/'\n      }, {\n        id: 3,\n        title: 'jjvy Artist Website',\n        image: project3,\n        desc: 'Personal Project: My other passion! Feel free to checkout my music!',\n        link: 'https://prodbyjjvy.vercel.app/#/'\n      }],\n      bottomProject: {\n        id: 4,\n        title: '33 Strategies',\n        image: project4,\n        desc: 'Personal Project: I read Robert Greene; felt like making a summarized website of the strategies; this is the result.',\n        link: 'https://33-strategies-of-war.now.sh/'\n      }\n    };\n  },\n  methods: {\n    sendEmail() {\n      window.location.href = 'mailto:jadonbguzman@tutanota.com';\n    }\n  }\n};","map":{"version":3,"names":["project1","project2","project3","project4","code","mail","designbanner","friendspicks","ProjectCard","FooterNav","name","components","data","projects","id","title","image","desc","link","bottomProject","methods","sendEmail","window","location","href"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- HERO AREA -->\n    <!-- DESKTOP VISIBILITY -->\n    <section data-aos=\"zoom-in\" class=\"my-5 d-none d-xl-block d-lg-block d-md-block d-md-block d-sm-none\">\n      <div class=\"d-flex flex-column align-items-center justify-content-center\">\n        <div class=\"d-flex flex-column align-items-xxl-start align-items-center text-xxl-start text-center w-25\">\n            <span class=\"span-color mb-2\">Jadon Guzman - Web Developer/Designer</span>\n            <div class=\"d-flex flex-row position-relative\">\n              <h1 class=\"fw-bold text-index\">Web Developer/Designer | Passion For The Arts in All Spheres<img width=\"15%\" :src=code alt=\"Code Icon\" /></h1>\n              <img class=\"position-absolute top-100 bottom-0 start-0 translate-middle banner-index\" width=\"120px\" :src=designbanner alt=\"Design Banner\" />\n            </div>\n        </div>\n        <div class=\"d-flex flex-column align-items-xxl-end justify-content-xxl-end align-items-center text-xxl-end text-center text-justify right-aligned w-25\">\n          <p class=\"grey-text text-index text-xxl-end text-center\">(Honestly, I just want to be in this field to fund my passions; just being honest! Regardless, web development is fun on occasion!)</p>\n          <button class=\"primary-button fw-medium\" @click=\"sendEmail\">\n            <img width=\"10%\" :src=mail alt=\"Mail Icon\" />\n            Contact\n          </button>\n        </div>\n      </div>\n    </section>\n    <!-- TABLET/ MOBILE VISIBILITY -->\n    <section data-aos=\"zoom-in\" class=\"my-5 container d-xl-none d-lg-none d-md-none w-100\">\n      <div class=\"d-flex flex-column justify-content-center align-items-center text-center\">\n        <span class=\"span-color mb-2\">John Doe - Web Developer/Designer</span>\n          <h2 class=\"fw-bold mobile-friendly\">Web Developer/Designer Turning Your Vision Into Business</h2>\n          <p class=\"grey-text text-center\">I help small businesses begin their marketing/branding campaigns & then implement a fully fleshed functional online store/website to industry standards</p>\n          <button class=\"primary-button fw-medium\" @click=\"sendEmail\">\n            <img width=\"10%\" :src=mail alt=\"Mail Icon\" />\n            contact@gmail.com\n          </button>\n        </div>\n    </section>\n    <!-- HERO AREA END -->\n\n    <!-- PROJECT SHOWCASE -->\n    <!-- DESKTOP VISIBILITY -->\n    <section class=\"container d-flex justify-content-center align-items-center mt-5 mx-5 flex-column d-none d-xl-block\">\n      <h1 data-aos=\"fade-right\"><span class=\"outline-text\">Project Showcase </span></h1>\n      <!-- TOP SECTION/GRID -->\n      <div class=\"project-container\">\n         <ProjectCard\n         data-aos=\"flip-down\"\n         v-for=\"project in projects\"\n         :key=\"project.id\"\n         :title=\"project.title\"\n         :image=\"project.image\"\n         :desc=\"project.desc\"\n         :link=\"project.link\"\n         />\n      </div>\n      <!-- BOTTOM SECTION -->\n      <div class=\"d-flex flex-row justify-content-center align-items-start bottom-container \">\n        <img data-aos=\"fade-right\" class=\"mb-5\" width=\"15%\" :src=friendspicks alt=\"Friends Picks Badge\" />\n        <ProjectCard\n        data-aos=\"flip-up\"\n        :key=\"bottomProject.id\"\n        :title=\"bottomProject.title\"\n        :image=\"bottomProject.image\"\n        :desc=\"bottomProject.desc\"\n        :link=\"bottomProject.link\"\n        />\n      </div>\n    </section>\n    <!-- TABLET - PHONE VISIBILITY -->\n    <section class=\"container d-flex justify-content-center align-items-center flex-column mx-auto d-xl-none\">\n      <h2 data-aos=\"fade-right\"><span class=\"outline-text\">Project Showcase </span></h2>\n      <!-- TOP SECTION/GRID -->\n      <div class=\"container d-flex justify-content-center align-items-center flex-row flex-wrap mt-5 mx-auto\">\n         <ProjectCard\n         data-aos=\"flip-down\"\n         v-for=\"project in projects\"\n         :key=\"project.id\"\n         :title=\"project.title\"\n         :image=\"project.image\"\n         :desc=\"project.desc\"\n         :link=\"project.link\"\n         />\n      </div>\n      <!-- BOTTOM SECTION -->\n      <div class=\"d-flex flex-row justify-content-center align-items-start\">\n        <img data-aos=\"fade-right\" class=\"mb-5 d-none d-lg-block d-xl-none img-fluid\" width=\"50%\" :src=friendspicks alt=\"Friends Picks Badge\" />\n        <ProjectCard\n        data-aos=\"flip-up\"\n        :key=\"bottomProject.id\"\n        :title=\"bottomProject.title\"\n        :image=\"bottomProject.image\"\n        :desc=\"bottomProject.desc\"\n        :link=\"bottomProject.link\"\n        />\n      </div>\n    </section>\n    <!-- PROJECT SHOWCASE END -->\n\n    <!-- CONTACT & SERVICES SECTION -->\n    <section class=\"container my-5\">\n      <h2 data-aos=\"fade-right\" class=\"text-lg-start text-center\"><span class=\"outline-text\">Contact & Services</span></h2>\n      <div class=\"d-flex flex-lg-row justify-content-lg-start align-items-lg-center flex-column justify-content-center align-items-center\">\n        <div data-aos=\"fade-up-left\" class=\"d-flex flex-column justify-content-lg-start align-items-lg-start justify-content-center align-items-center text-lg-start text-center colored-card m-5 p-4\">\n          <h4 class=\"text-dark\">I`m Building UI Layouts & Functional Interfaces </h4>\n          <p class=\"text-dark fw-normal\">Focused on functional, but fluid UI/UX experiences for startups to thriving businesses</p>\n          <hr class=\"text-dark\" width=\"100%\">\n          <button class='primary-button fw-medium'>About</button>\n        </div>\n        <div data-aos=\"fade-up-right\" class=\"w-50 d-flex flex-column justify-content-lg-start align-items-lg-start justify-content-center align-items-center text-lg-start text-center\">\n          <span class=\"span-color\">Contact</span>\n          <h2>Want To Learn More? Let`s Get Started</h2>\n          <p>feel free to reach out on Linkedin, Twitter or Instagram!</p>\n          <button class=\"primary-button fw-medium\" @click=\"sendEmail\">\n            <img width=\"10%\" :src=mail alt=\"Mail Icon\" />\n            contact@gmail.com\n          </button>\n        </div>\n      </div>\n    </section>\n    <!-- CONTACT & SERVICES SECTION END -->\n    \n    <!-- FOOTER -->\n    <FooterNav />\n  </div>\n</template>\n\n<script>\n// IMAGES\nimport project1 from '../assets/project1.png';\nimport project2 from '../assets/project2.png';\nimport project3 from '../assets/project3.png';\nimport project4 from '../assets/project4.png';\nimport code from '../assets/Code.svg';\nimport mail from '../assets/mail.svg';\nimport designbanner from '../assets/design-banner.svg';\nimport friendspicks from '../assets/FriendsPicks.svg';\n// COMPONENTS\nimport ProjectCard from '../components/Project-Card.vue';\nimport FooterNav from '../components/FooterNav.vue';\n\nexport default {\n  name: \"HomeView\",\n  components: {\n    ProjectCard,\n    FooterNav\n  },\n  data() {\n    return {\n      // IMAGE DATA\n      code: code,\n      mail: mail,\n      designbanner: designbanner,\n      friendspicks: friendspicks,\n\n      // PROJECT DATA\n      projects: [\n        {\n          id: 1,\n          title: 'Web Agency',\n          image: project1,\n          desc: 'Personal Project: Local Seattle agency inspired me, so I felt like doing a redesign of their website.',\n          link: 'https://web-agency-vs7d.vercel.app/'\n        },\n        {\n          id: 2,\n          title: 'Achilles Collective',\n          image: project2,\n          desc: `Personal Project: A Meetup Group passion project that's one of a couple reasons why I'm trying so hard to get into the I.T industry at the moment; still in the works, but I plan on investing more into this project as a whole once I'm more settled into my career (please hire me lol (I'll train you for free!!))`,\n          link: 'https://achilles-collective.vercel.app/'\n        },\n        {\n          id: 3,\n          title: 'jjvy Artist Website',\n          image: project3,\n          desc: 'Personal Project: My other passion! Feel free to checkout my music!',\n          link: 'https://prodbyjjvy.vercel.app/#/'\n        }\n      ],\n      bottomProject: {\n          id: 4,\n          title: '33 Strategies',\n          image: project4,\n          desc: 'Personal Project: I read Robert Greene; felt like making a summarized website of the strategies; this is the result.',\n          link: 'https://33-strategies-of-war.now.sh/'\n        },\n    }\n  },\n  methods: {\n    sendEmail() {\n      window.location.href = 'mailto:jadonbguzman@tutanota.com';\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* HERO AREA TEXT */\n.right-aligned {\n  text-align: right !important;\n}\n\n.text-index {\n  z-index: 1;\n}\n\n.banner-index {\n  z-index: 0;\n}\n\n.mobile-friendly {\n  font-size: 2rem !important;\n}\n\n/* PROJECT CONTAINER */\n.project-container {\n  justify-content: center;\n  align-items: center;\n  display: grid;\n  column-gap: 50px;\n  row-gap: 50px;\n  justify-content: center;\n}\n.project-container div:nth-child(1) {\n  /* width: 70%; */\n  grid-column-start: 1;\n  grid-column-end: 2;\n  grid-row-start: 1;\n  grid-row-end: 1;\n}\n\n.project-container div:nth-child(2) {\n  /* width: 80%; */\n  grid-column-start: 2;\n  grid-column-end: 2;\n  grid-row-start: 1;\n  grid-row-end: 2;\n}\n\n.project-container div:nth-child(3) {\n  width: 50%;\n  grid-column-start: 1;\n  grid-column-end: 3;\n  grid-row-start: 2;\n  grid-row-end: 2;\n  place-self: center;\n}\n\n.project-container div:nth-child(4) {\n  /* width: 30%; */\n  grid-column-start: 1;\n  grid-column-end: 2;\n  grid-row-start: 3;\n  grid-row-end: 3;\n}\n\n/* BOTTOM PROJECT CONTAINER */\n.bottom-container {\n  margin-top: 50px;\n  margin-bottom: 50px;\n}\n.bottom-container div {\n  width: 40%;\n}\n</style>\n"],"mappings":"AA4HA;AACA,OAAAA,QAAA;AACA,OAAAC,QAAA;AACA,OAAAC,QAAA;AACA,OAAAC,QAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,YAAA;AACA,OAAAC,YAAA;AACA;AACA,OAAAC,WAAA;AACA,OAAAC,SAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH,WAAA;IACAC;EACA;EACAG,KAAA;IACA;MACA;MACAR,IAAA,EAAAA,IAAA;MACAC,IAAA,EAAAA,IAAA;MACAC,YAAA,EAAAA,YAAA;MACAC,YAAA,EAAAA,YAAA;MAEA;MACAM,QAAA,GACA;QACAC,EAAA;QACAC,KAAA;QACAC,KAAA,EAAAhB,QAAA;QACAiB,IAAA;QACAC,IAAA;MACA,GACA;QACAJ,EAAA;QACAC,KAAA;QACAC,KAAA,EAAAf,QAAA;QACAgB,IAAA;QACAC,IAAA;MACA,GACA;QACAJ,EAAA;QACAC,KAAA;QACAC,KAAA,EAAAd,QAAA;QACAe,IAAA;QACAC,IAAA;MACA,EACA;MACAC,aAAA;QACAL,EAAA;QACAC,KAAA;QACAC,KAAA,EAAAb,QAAA;QACAc,IAAA;QACAC,IAAA;MACA;IACA;EACA;EACAE,OAAA;IACAC,UAAA;MACAC,MAAA,CAAAC,QAAA,CAAAC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}